    "@storybook/svelte": "^6.4.22",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.5.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
